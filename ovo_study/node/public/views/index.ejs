<!DOCTYPE html>
<html lang="ko">
    <head>
        <title> User list </title>
    </head>
    <body>
        <table>
            <tr>
                <th>아이디</th>
                <th>이름</th>
                <th>나이</th>
                <th>비밀번호</th>
                <th>수정</th>
                <th>삭제</th>
            </tr>
            <% user.forEach(function (value){ %>
                <tr>
                    <td><%= value.id %></td>
                    <td><%= value.name %></td>
                    <td><%= value.age %></td>
                    <td><%= value.password %></td>
                    <td><a href="/edit/<%= value.id %>">수정</a></td>
                    <td><a href="/delete/<%= value.id %>">삭제</a></td>
                </tr>
                <% }); %>
        </table>
    </body>
</html>